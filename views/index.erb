<h1> Student Register</h1>
<hr/>
<% @students.each do |student| %>
  <p>First Name: <%= student.first_name%> </p>
  <p>Last Name: <%= student.last_name%> </p>
  <p>House: <%= student.house%> </p>
  <p>Age: <%= student.age%></p>
  <p><a href="/hogwarts/<%=student.id%>">View Student</a></p>

<hr/>
<%end%>
